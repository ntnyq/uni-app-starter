{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "prettier.configPath": "./prettier.config.mjs",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "files.associations": {
    "pages.json": "jsonc"
  },
  "eslint.validate": [
    "vue",
    "xml",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "armeabi",
    "automator",
    "bumpp",
    "changelogithub",
    "dcloudio",
    "Echarts",
    "EFABD",
    "esbuild",
    "iconify",
    "INTLIFY",
    "kuaishou",
    "nocheck",
    "nvmrc",
    "nvue",
    "pinia",
    "swiper",
    "toutiao",
    "unplugin",
    "Unref",
    "unrs",
    "vite",
    "vueuse",
    "weixin"
  ]
}
