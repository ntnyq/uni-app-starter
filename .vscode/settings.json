{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "xml",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "armeabi",
    "automator",
    "bumpp",
    "changelogithub",
    "dcloudio",
    "iconify",
    "INTLIFY",
    "kuaishou",
    "nocheck",
    "nvmrc",
    "nvue",
    "pinia",
    "swiper",
    "toutiao",
    "unplugin",
    "vite",
    "vueuse",
    "weixin"
  ]
}
