      "esbuild",
      "vue-demi"
    ]
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,mts,vue,svg,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
