    "@unocss/reset": "catalog:",
    "@vueuse/core": "^12.8.2",
    "dayjs": "^1.11.13",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "wot-design-uni": "^1.7.1"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.14",
    "@dcloudio/uni-automator": "catalog:",
