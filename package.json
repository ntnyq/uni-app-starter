    "@uni-helper/vite-plugin-uni-platform": "^0.0.4",
    "@vue/runtime-core": "^3.5.13",
    "autoprefixer": "^10.4.20",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
